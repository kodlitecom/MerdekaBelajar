<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pintar.merdekabelajar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MerdekaBelajar">
        <activity android:name=".DetailKategoriActivity"></activity>
        <activity android:name=".DetailKursusActivity" />
        <activity android:name=".profile.EditprofilActivity" />
        <activity android:name=".menu.HelpActivity" />
        <activity android:name=".menu.FaqActivity" />
        <activity android:name=".menu.AboutActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".profile.ProfileActivity" />
        <activity android:name=".authentication.ResetActivity" />
        <activity android:name=".authentication.RegisterActivity" />
        <activity android:name=".authentication.LoginActivity" />
        <activity android:name=".authentication.HelloActivity" />
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden" />
    </application>

</manifest>